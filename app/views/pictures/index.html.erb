<h1>All pictures</h1>
<% @pictures.each do |picture| %>
	<div class="row">
		<div class="col-md-3">
			Title: <%= picture.title %>
		</div>
	</div>
	<div class="row">
		<div class="col-md-3">
			<%= link_to image_tag(picture.image.url(:medium)), picture_path(picture) %>
		</div>
	</div>
	<div class="row">
		<div class="col-md-3">
			Author: <%= picture.user.name %>
		</div>
	</div>
<% end %>